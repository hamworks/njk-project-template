{
  "name": "njk-project-template",
  "version": "1.0.0",
  "description": "Eleventy + Vite + Nunjucks static site generator template",
  "type": "module",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "eleventy",
    "preview": "eleventy --serve",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "volta": {
    "node": "24.11.1",
    "npm": "11.6.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-vite": "^7.0.0",
    "@biomejs/biome": "^2.0.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.0.0",
    "@types/node": "^22.0.0",
    "jsdom": "^26.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
